<ivy-module version="2.0">
    <info organisation="ir_service" module="ir_service"/>
    <dependencies>

        <dependency org="ir_hibernate" name="ir_hibernate" rev="2.2" changing="true" />
        
         <!--  digester dependency -->    
        <dependency org="commons-digester" name="commons-digester" rev="2.0" transitive="false">
            <artifact name="commons-digester" type="jar"/>
        </dependency>
        
  
        <!-- ehcahce -->
        <dependency org="net.sf.ehcache" name="ehcache" rev="1.6.0" transitive="false">
            <artifact name="ehcache" type="jar"/>
        </dependency>

         <!-- lucene files-->
        <dependency org="org.apache.lucene" name="lucene-misc" rev="2.4.1">
            <artifact name="lucene-misc" type="jar"/>
        </dependency>
        
        <!-- spring files -->
        <dependency org="org.springframework" name="spring-context-support" rev="3.0.2.RELEASE" transitive="false">
            <artifact name="spring-context-support" type="jar"/>
        </dependency>
        
 
        <!--  quartz files  -->
        <dependency org="org.quartz-scheduler" name="quartz" rev="1.8.3" transitive="false">
            <artifact name="quartz" type="jar"/>
        </dependency>
        
        <dependency org="com.sun.activation" name="javax.activation" rev="1.2.0" transitive="false" >
         <artifact name="javax.activation" type="jar"/>
        </dependency>
        
    </dependencies>
</ivy-module>
